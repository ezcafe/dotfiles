{
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json][jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript][javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript][typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "conventionalCommits.autoCommit": false,
  "conventionalCommits.emojiFormat": "emoji",
  "conventionalCommits.lineBreak": "\\n",
  "conventionalCommits.promptBody": false,
  "conventionalCommits.scopes": ["a11y", "config", "tests"],
  "editor.detectIndentation": true,
  "editor.fontSize": 16,
  "editor.formatOnSaveMode": "modifications",
  "editor.guides.bracketPairs": "active",
  "editor.minimap.enabled": false,
  "editor.rulers": [80, 100],
  "files.trimTrailingWhitespace": true,
  "git.autofetch": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "oneDarkPro.bold": true,
  "oneDarkPro.editorTheme": "One Dark Pro Flat",
  "telemetry.telemetryLevel": "off",
  "terminal.integrated.fontSize": 14,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "workbench.colorCustomizations": {
    "editorBracketPairGuide.background1": "#F8B46C",
    "editorBracketPairGuide.background2": "#FB78B5",
    "editorBracketPairGuide.background3": "#4AACFB",
    "editorBracketPairGuide.background4": "#B98BF5",
    "editorBracketPairGuide.background5": "#DAD8D8",
    "editorBracketPairGuide.background6": "#18EECE",
    "editorBracketPairGuide.activeBackground1": "#F367C9",
    "editorBracketPairGuide.activeBackground2": "#FD70B2",
    "editorBracketPairGuide.activeBackground3": "#41A7FA",
    "editorBracketPairGuide.activeBackground4": "#B084EB",
    "editorBracketPairGuide.activeBackground5": "#DBDADA",
    "editorBracketPairGuide.activeBackground6": "#16FAD8"
  },
  "workbench.colorTheme": "One Dark Pro Darker",
  "workbench.layoutControl.enabled": false,
  "window.commandCenter": true,
  "workbench.startupEditor": "none",
  "editor.tabSize": 2,
  "editor.fontFamily": "Fira Code, Menlo, Monaco, 'Courier New', monospace",
  "editor.fontLigatures": true
}
