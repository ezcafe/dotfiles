alias gl="git log --oneline --all --graph --decorate  $(git reflog | awk '{print $1}')"
alias gll="git log --graph --abbrev-commit --date=relative --pretty=format:'%C(red)%h%Creset %s - %C(yellow)%an%Creset%C(red)%d%Creset %C(green)%cr%Creset'"

alias gp="git pull"
alias gpr="git pull --rebase"
alias gpra="git rebase --abort"
alias gprc="git rebase --continue"
alias gs="git rebase -i HEAD~" # Squash gprs 2

